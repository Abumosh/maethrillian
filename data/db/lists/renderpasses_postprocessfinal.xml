<RenderPasses>
    <SetViewport group="PostProcessing">
        <Coords x="0" y="0" relativeWidth="1" relativeHeight="1">
        </Coords>
        <Depth minZ="0" maxZ="1">
        </Depth>
    </SetViewport>
    <SetScissorRect group="PostProcessing">
        <Coords x="0" y="0" relativeWidth="1" relativeHeight="1">
        </Coords>
    </SetScissorRect>
    <ConditionalPass group="PostProcessing">
        <If condition="HDREnabled">
            <ConditionalPass group="Postprocessing">
                <If condition="SMAAEnabled">
                    <SetRenderTargets group="PostProcessing">
                        <Target name="PostProcessFinal">
                        </Target>
                        <Target name="PostProcessFinalHDRPreAA">
                        </Target>
                    </SetRenderTargets>
                </If>
                <Else>
                    <SetRenderTargets group="PostProcessing">
                        <Target name="PostProcessFinal">
                        </Target>
                        <Target name="PostProcessFinalHDR">
                        </Target>
                    </SetRenderTargets>
                </Else>
            </ConditionalPass>
        </If>
        <Else>
            <SetRenderTargets group="PostProcessing">
                <Target name="PostProcessFinal">
                </Target>
            </SetRenderTargets>
        </Else>
    </ConditionalPass>
    <SetTexture group="PostProcessing">
        <Target name="LightAccumulation">
        </Target>
        <Shader type="Pixel">
        </Shader>
        <Index index="0">
        </Index>
    </SetTexture>
    <SamplerState group="PostProcessing">
        <Address u="Mirror" v="Mirror" w="Mirror">
        </Address>
        <Filter min="Linear" mag="Linear" mip="Linear">
        </Filter>
        <Index index="0">
        </Index>
    </SamplerState>
    <SetTexture group="PostProcessing">
        <Target name="DistortionTarget">
        </Target>
        <Shader type="Pixel">
        </Shader>
        <Index index="1">
        </Index>
    </SetTexture>
    <SamplerState group="PostProcessing">
        <Address u="Clamp" v="Clamp" w="Clamp">
        </Address>
        <Filter min="Linear" mag="Linear" mip="Linear">
        </Filter>
        <Index index="1">
        </Index>
    </SamplerState>
    <SetTexture group="PostProcessing">
        <Target name="Bloom4x4">
        </Target>
        <Shader type="Pixel">
        </Shader>
        <Index index="2">
        </Index>
    </SetTexture>
    <SamplerState group="PostProcessing">
        <Address u="Clamp" v="Clamp" w="Clamp">
        </Address>
        <Filter min="Linear" mag="Linear" mip="Linear">
        </Filter>
        <Index index="2">
        </Index>
    </SamplerState>
    <SetTexture group="PostProcessing">
        <RenderGlobal name="SceneLUT">
        </RenderGlobal>
        <Shader type="Pixel">
        </Shader>
        <Index index="3">
        </Index>
    </SetTexture>
    <SetTexture group="PostProcessing">
        <Target name="BlurWideOutput">
        </Target>
        <Shader type="Pixel">
        </Shader>
        <Index index="4">
        </Index>
    </SetTexture>
    <SamplerState group="PostProcessing">
        <Address u="Clamp" v="Clamp" w="Clamp">
        </Address>
        <Filter min="Linear" mag="Linear" mip="Linear">
        </Filter>
        <Index index="4">
        </Index>
    </SamplerState>
    <SamplerState group="PostProcessing">
        <Address u="Clamp" v="Clamp" w="Clamp">
        </Address>
        <Filter min="Linear" mag="Linear" mip="Linear">
        </Filter>
        <Index index="3">
        </Index>
    </SamplerState>
    <SetPostProcessingShader group="PostProcessing">
    </SetPostProcessingShader>
    <SetTexture group="PostProcessing">
        <RenderGlobal name="TonemapData">
        </RenderGlobal>
        <Shader type="Pixel">
        </Shader>
        <Index index="5">
        </Index>
    </SetTexture>
    <SetTexture group="PostProcessing">
        <Target name="VisibilityTextureCurr">
        </Target>
        <Shader type="Pixel">
        </Shader>
        <Index index="11">
        </Index>
    </SetTexture>
    <SamplerState group="PostProcessing">
        <Address u="Clamp" v="Clamp" w="Clamp">
        </Address>
        <Filter min="Linear" mag="Linear" mip="Linear">
        </Filter>
        <Index index="11">
        </Index>
    </SamplerState>
    <SetTexture group="PostProcessing">
        <Target name="VisibilityTexturePrev">
        </Target>
        <Shader type="Pixel">
        </Shader>
        <Index index="13">
        </Index>
    </SetTexture>
    <SamplerState group="PostProcessing">
        <Address u="Clamp" v="Clamp" w="Clamp">
        </Address>
        <Filter min="Linear" mag="Linear" mip="Linear">
        </Filter>
        <Index index="13">
        </Index>
    </SamplerState>
    <SetTexture group="PostProcessing">
        <Target name="DepthCopy">
        </Target>
        <Shader type="Pixel">
        </Shader>
        <Index index="7">
        </Index>
    </SetTexture>
    <SamplerState group="PostProcessing">
        <Address u="Clamp" v="Clamp" w="Clamp">
        </Address>
        <Filter min="Point" mag="Point" mip="Point">
        </Filter>
        <Index index="7">
        </Index>
    </SamplerState>
    <SetTexture group="PostProcessing">
        <RenderGlobal name="PlayableBoundaryTexture">
        </RenderGlobal>
        <Shader type="Pixel">
        </Shader>
        <Index index="14">
        </Index>
    </SetTexture>
    <SamplerState group="PostProcessing">
        <Address u="Clamp" v="Clamp" w="Clamp">
        </Address>
        <Filter min="Linear" mag="Linear" mip="Linear">
        </Filter>
        <Index index="14">
        </Index>
    </SamplerState>
    <DrawNoVertexBuffer group="PostProcessing">
        <Primitive type="TriList" count="1">
        </Primitive>
    </DrawNoVertexBuffer>
    <SetTexture group="Lensflares">
        <Target name="DepthCopy">
        </Target>
        <Shader type="Compute">
        </Shader>
        <Index index="7">
        </Index>
    </SetTexture>
    <SamplerState group="Lensflares">
        <Address u="Clamp" v="Clamp" w="Clamp">
        </Address>
        <Filter min="Point" mag="Point" mip="Point">
        </Filter>
        <Index index="7">
        </Index>
        <Shader type="Compute">
        </Shader>
    </SamplerState>
    <SetTexture group="Lensflares">
        <Target name="VisibilityTextureCurr">
        </Target>
        <Shader type="Compute">
        </Shader>
        <Index index="11">
        </Index>
    </SetTexture>
    <SamplerState group="Lensflares">
        <Address u="Clamp" v="Clamp" w="Clamp">
        </Address>
        <Filter min="Linear" mag="Linear" mip="Linear">
        </Filter>
        <Shader type="Compute">
        </Shader>
        <Index index="11">
        </Index>
    </SamplerState>
    <SetTexture group="Lensflares">
        <Target name="VisibilityTexturePrev">
        </Target>
        <Shader type="Compute">
        </Shader>
        <Index index="13">
        </Index>
    </SetTexture>
    <SamplerState group="Lensflares">
        <Address u="Clamp" v="Clamp" w="Clamp">
        </Address>
        <Filter min="Linear" mag="Linear" mip="Linear">
        </Filter>
        <Shader type="Compute">
        </Shader>
        <Index index="13">
        </Index>
    </SamplerState>
    <LensFlareRender group="Lensflares">
    </LensFlareRender>
    <ConditionalPass group="PostProcessing">
        <If condition="SMAAEnabled">
        </If>
        <Else>
            <SetBackbufferRenderTarget group="PostProcessing">
            </SetBackbufferRenderTarget>
            <SetTexture group="PostProcessing">
                <Target name="PostProcessFinal">
                </Target>
                <Shader type="Pixel">
                </Shader>
                <Index index="0">
                </Index>
            </SetTexture>
            <SamplerState group="PostProcessing">
                <Address u="Clamp" v="Clamp" w="Clamp">
                </Address>
                <Filter min="Point" mag="Point" mip="Point">
                </Filter>
                <Index index="0">
                </Index>
            </SamplerState>
            <SetShader group="PostProcessing">
                <Shader name="PassThrough8bpc">
                </Shader>
            </SetShader>
            <DrawNoVertexBuffer group="PostProcessing">
                <Primitive type="TriList" count="1">
                </Primitive>
            </DrawNoVertexBuffer>
        </Else>
    </ConditionalPass>
</RenderPasses>